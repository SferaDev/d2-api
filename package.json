{
    "name": "@eyeseetea/d2-api",
    "description": "Typed wrapper over DHIS2 API",
    "version": "1.8.5",
    "license": "GPL-3.0",
    "author": "EyeSeeTea team",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eyeseetea/d2-api.git"
    },
    "scripts": {
        "start": "NODE_ENV=development babel-watch src --extensions \".js,.jsx,.ts,.tsx\"",
        "clean": "rimraf build",
        "build-babel": "babel src --out-dir build --extensions \".js,.jsx,.ts,.tsx\"",
        "build": "yarn build-babel && tsc && cp package.json build/",
        "predist": "rimraf dist && yarn build",
        "dist": "ncc build build/ -m",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "postdist": "cp dist/index.js $npm_package_name.js",
        "prettify": "prettier \"{.,src}/**/*.{js,jsx,ts,tsx,json,css}\" --write",
        "generate-schemas": "ts-node src/scripts/generate-schemas.ts",
        "prerelease": "yarn build",
        "release": "bash scripts/publish.sh"
    },
    "dependencies": {
        "@babel/runtime": "7.14.6",
        "@dhis2/app-service-data": "2.8.0",
        "@dhis2/d2-i18n": "1.1.0",
        "@types/prettier": "2.3.0",
        "@types/qs": "6.9.6",
        "abort-controller": "3.0.0",
        "argparse": "2.0.1",
        "axios": "0.21.1",
        "axios-debug-log": "0.8.2",
        "axios-mock-adapter": "1.19.0",
        "btoa": "1.2.1",
        "cronstrue": "1.114.0",
        "cryptr": "6.0.2",
        "d2": "31.10.0",
        "dotenv": "10.0.0",
        "express": "4.17.1",
        "iconv-lite": "0.6.3",
        "isomorphic-fetch": "3.0.0",
        "lodash": "4.17.21",
        "log4js": "6.3.0",
        "node-schedule": "2.0.0",
        "qs": "6.10.1",
        "react": "17.0.2",
        "yargs": "17.0.1"
    },
    "devDependencies": {
        "@babel/cli": "7.14.5",
        "@babel/core": "7.14.6",
        "@babel/node": "7.14.5",
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@babel/plugin-proposal-decorators": "7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "7.14.5",
        "@babel/plugin-transform-runtime": "7.14.5",
        "@babel/preset-env": "7.14.5",
        "@babel/preset-typescript": "7.14.5",
        "@types/argparse": "2.0.8",
        "@types/btoa": "1.2.3",
        "@types/cryptr": "4.0.1",
        "@types/express": "4.17.12",
        "@types/lodash": "4.14.170",
        "@types/node": "15.12.4",
        "@types/node-schedule": "1.3.1",
        "@types/react": "17.0.11",
        "@types/yargs": "17.0.0",
        "@typescript-eslint/eslint-plugin": "4.27.0",
        "@typescript-eslint/parser": "4.27.0",
        "@zeit/ncc": "0.20.4",
        "babel-eslint": "10.0.2",
        "babel-watch": "7.4.1",
        "eslint": "7.29.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.24.0",
        "prettier": "2.3.1",
        "ts-node": "10.0.0",
        "typescript": "4.3.4",
        "watch": "1.0.2"
    }
}
